{
	"apiVersion": 2,
	"name": "postnl-for-woocommerce/postnl-tabs-block",
	"version": "2.0.0",
	"title": "Alternative Shipping Options Block",
	"category": "woocommerce",
	"description": "Adds a select field to let the shopper choose alternative shipping instructions.",
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [ "woocommerce/checkout-order-summary-block" ],
	"attributes": {
        "tabs": {
            "type": "array",
            "default": [
                { "id": "tab1", "name": "Option 1" },
                { "id": "tab2", "name": "Option 2" }
            ]
        },
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		},
		"letterbox": {
            "type": "boolean",
            "default": false
        },
        "field": {
            "type": "object",
            "default": {
                "value": "test",
                "name": "postnl_shipping_option"
            }
        },
        "defaultVal": {
            "type": "object",
            "default": {
                "val": "1",
                "date": "2",
                "from": "3",
                "to": "4",
                "price": "5",
                "type": "6"
            }
        },
        "response": {
            "type": "object",
            "default": {}
        },
        "postData": {
            "type": "object",
            "default": {}
        }
	},
	"textdomain": "postnl-for-woocommerce",
	"editorStyle": "file:../../../build/style-shipping-workshop-block.css"
}
